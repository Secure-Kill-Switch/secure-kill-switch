{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "experimentalUI": true,
  "globalEnv": ["POSTGRES_URL", "POSTGRES_URL_NON_POOLING"],
  "pipeline": {
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prebuild": {},
    "generate": {
      "dependsOn": ["^generate"]
    },
    "migrate": {
      "cache": false,
      "interactive": true
    },
    "clean": {
      "cache": false
    }
  }
}
